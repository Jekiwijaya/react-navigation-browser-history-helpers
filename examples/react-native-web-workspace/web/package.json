  "scripts": {
    "web": "webpack-dev-server --config webpack.dev.config.js",
    "start": "node server.js",
    "build": "webpack --config webpack.prod.config.js",
    "remove:root:build": "rm ../../../bundle.js",
    "copy:root:build": "mv build/bundle.js ../../../bundle.js",
